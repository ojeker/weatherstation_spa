{
  "name": "weatherstation_spa",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:e2e": "npm run build && start-server-and-test 'npm run preview -- --port 5173' http://localhost:5173 'cypress run'",
    "test:e2e:dev": "start-server-and-test 'npm run dev' http://localhost:5173 'cypress run'",
    "test:e2e:open": "start-server-and-test 'npm run dev' http://localhost:5173 'cypress open'",
    "lint": "eslint src tests --ext .ts,.vue",
    "lint:fix": "eslint src tests --ext .ts,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\" \"tests/**/*.ts\""
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.0"
  }
}
